{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "jsx": "react-native",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "outDir": "dist",
    "types": [
      "react",
      "react-native",
      "prop-types",
      "node"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "src/**/*",
    "App.tsx"
, "expo-env.d.ts"  ],
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "webpack.config.js",
    "dist"
  ],
  "extends": "expo/tsconfig.base"
}
